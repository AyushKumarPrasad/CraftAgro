                    android:layout_height="40dp"
                    android:ems="10"
                    android:inputType="number"
                    android:textColor="#ffffff"
                    android:textColorHint="#ffffff"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText,LabelFor,SmallSp" />

                <EditText
                    android:id="@+id/editText4"
                    android:layout_gravity="center"
                    android:layout_width="30dp"
                    android:layout_height="40dp"
                    android:ems="10"

                    android:inputType="number"
                    android:textColor="#ffffff"
                    android:textColorHint="#ffffff"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText,LabelFor,SmallSp"/>

                <EditText
                    android:id="@+id/editText5"
                    android:layout_gravity="center"
                    android:layout_width="30dp"
                    android:layout_height="40dp"
                    android:ems="10"
                    android:inputType="number"
                    android:textColor="#ffffff"
                    android:textColorHint="#ffffff"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText,LabelFor,SmallSp" />

                <EditText
                    android:id="@+id/editText6"
                    android:layout_gravity="center"
                    android:layout_width="30dp"
                    android:layout_height="40dp"
                    android:ems="10"
                    android:inputType="number"
                    android:textColor="#ffffff"
                    android:textColorHint="#ffffff"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText,LabelFor,SmallSp" />

                <EditText
                    android:id="@+id/editText7"
                    android:layout_gravity="center"
                    android:layout_width="30dp"
                    android:layout_height="40dp"
                    android:ems="10"
                    android:inputType="number"
                    android:textColor="#ffffff"
                    android:textColorHint="#ffffff"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText,LabelFor,SmallSp" />

                <EditText
                    android:id="@+id/editText8"
                    android:layout_gravity="center"
                    android:layout_width="30dp"
                    android:layout_height="40dp"
                    android:ems="10"
                    android:inputType="number"
                    android:textColor="#ffffff"
                    android:textColorHint="#ffffff"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText,LabelFor,SmallSp"/>

                <EditText
                    android:id="@+id/editText9"
                    android:layout_width="30dp"
                    android:layout_gravity="center"
                    android:layout_height="40dp"
                    android:ems="10"
                    android:inputType="number"
                    android:textColor="#ffffff"
                    android:textColorHint="#ffffff"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText,LabelFor,SmallSp" />
            </LinearLayout>


            <LinearLayout

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textview_payment"
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Pay"
                    android:textColor="#ffffff"
                    android:textSize="12sp"
                    android:textStyle="bold"
                    tools:ignore="HardcodedText" />

                <TextView
                    android:id="@+id/pricetext1"
                    android:gravity="center"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:ems="10"
                    android:hint="0"
                    android:inputType="number"
                    android:textColor="#ffffff"
                    android:textColorHint="#ffffff"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText,LabelFor,SmallSp" />

                <TextView
                    android:id="@+id/pricetext2"
                    android:gravity="center"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:ems="10"
                    android:hint="0"
                    android:inputType="number"
                    android:textColor="#ffffff"
                    android:textColorHint="#ffffff"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText,LabelFor,SmallSp" />

                <TextView
                    android:id="@+id/pricetext3"
                    android:gravity="center"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:ems="10"
                    android:hint="0"
                    android:inputType="number"
                    android:textColor="#ffffff"
                    android:textColorHint="#ffffff"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText,LabelFor,SmallSp" />

                <TextView
                    android:id="@+id/pricetext4"
                    android:gravity="center"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:ems="10"
                    android:hint="0"
                    android:inputType="number"
                    android:textColor="#ffffff"
                    android:textColorHint="#ffffff"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText,LabelFor,SmallSp" />

                <TextView
                    android:id="@+id/pricetext5"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:ems="10"
                    android:hint="0"
                    android:inputType="number"
                    android:textColor="#ffffff"
                    android:textColorHint="#ffffff"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText,LabelFor,SmallSp" />

                <TextView
                    android:id="@+id/pricetext6"
                    android:gravity="center"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:ems="10"
                    android:hint="0"
                    android:inputType="number"
                    android:textColor="#ffffff"
