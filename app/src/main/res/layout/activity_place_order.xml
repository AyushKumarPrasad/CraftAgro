<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/relativelayout_placeorder"
    android:padding="2dp"
    android:background="#006400"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:card_view="http://schemas.android.com/tools"
    tools:context=".PlaceOrder">

    <android.support.v7.widget.CardView
        android:layout_marginTop="1dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:id="@+id/card1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:cardBackgroundColor="#708238"
        app:cardCornerRadius="20dp"
        card_view:cardUseCompatPadding="true"
        card_view:card_Elevation="35dp">
        <LinearLayout
            android:id="@+id/l1"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/l2"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/back_button_place_order"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp"
                    android:src="@drawable/leftarrowwhite" />
                <EditText
                    android:id="@+id/name"
                    android:digits="abcdefghijklmnopqrst
                    uvwxyzABC
                    DEFGHIJKLMNOPQRSTUVWXYZ"
                    android:layout_width="115dp"
                    android:layout_height="40dp"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/toolbar22"
                    android:layout_marginLeft="50dp"
                    android:ems="10"
                    android:hint="name"
                    android:inputType="textPersonName"
                    android:textAlignment="center"
                    android:textColor="#ffffff"
                    android:textColorHint="#ffffff"
                    android:textSize="14sp"
                    tools:ignore="HardcodedText,LabelFor,
                    UnknownIdInLayout" />
                <EditText
                    android:id="@+id/phone"
                    android:layout_width="115dp"
                    android:layout_height="40dp"
                    android:ems="10"
                    android:hint="phone  no"
                    android:inputType="number"
                    android:maxLength="10"
                    android:textAlignment="center"
                    android:textColor="#ffffff"
                    android:textColorHint="#ffffff"
                    android:textSize="14sp"
                    tools:ignore="HardcodedText,LabelFor,
                    TextFields,UnknownId" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/l3"
            android:layout_marginTop="50dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <EditText
                android:id="@+id/address"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/phone"
                android:layout_marginLeft="100dp"
                android:layout_marginRight="5dp"
                android:ems="10"
                android:hint="address"
                android:inputType="textPersonName"
                android:textAlignment="center"
                android:textColor="#ffffff"
                android:textColorHint="#ffffff"
                android:textSize="14sp"
                tools:ignore="HardcodedText,LabelFor" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/linearspinner"
            android:layout_marginTop="95dp"
            android:gravity="center"
            android:background="#708238"
            android:layout_below="@+id/linear1"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <Spinner
                android:id="@+id/spinner_vegetable_list"
                android:layout_width="230dp"
                android:layout_height="40dp"
                android:layout_below="@+id/linear1"
                android:popupBackground="#708238"
                android:textSize="14sp" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/linear2"
            android:layout_marginTop="135dp"
            android:background="#708238"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/linearspinner"
            android:orientation="horizontal">

            <LinearLayout
