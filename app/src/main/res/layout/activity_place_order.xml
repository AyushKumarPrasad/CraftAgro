                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"

                    android:text="Vegetables"
                    android:textColor="#ffffff"
                    android:textSize="12sp"
                    android:textStyle="bold"
                    tools:ignore="HardcodedText"  />

                <CheckBox
                    android:id="@+id/check1"
                    android:layout_width="100dp"
                    android:layout_height="40dp"

                    android:text="Bak Choy"
                    android:textColor="#ffffff"
                    android:textSize="10sp"
                    android:textStyle="bold" />

                <CheckBox
                    android:id="@+id/check2"
                    android:layout_width="100dp"
                    android:layout_height="40dp"
                    android:text="Spinach"
                    android:textColor="#ffffff"
                    android:textSize="10sp"
                    android:textStyle="bold" />

                <CheckBox
                    android:id="@+id/check3"
                    android:layout_width="100dp"
                    android:layout_height="40dp"
                    android:text="Lettuce Lollo Rosso"
                    android:textColor="#ffffff"
                    android:textSize="10sp"
                    android:textStyle="bold" />

                <CheckBox
                    android:id="@+id/check4"
                    android:layout_width="100dp"
                    android:layout_height="40dp"
                    android:text="Lettuce Marvel of four Seasons"
                    android:textColor="#ffffff"
                    android:textSize="10sp"
                    android:textStyle="bold" />

                <CheckBox
                    android:id="@+id/check5"
                    android:layout_width="100dp"
                    android:layout_height="40dp"
                    android:text="Kale"
                    android:textColor="#ffffff"
                    android:textSize="10sp"
                    android:textStyle="bold" />

                <CheckBox
                    android:id="@+id/check6"
                    android:layout_width="100dp"
                    android:layout_height="40dp"
                    android:text="Mustard Greens"
                    android:textColor="#ffffff"
                    android:textSize="10sp"
                    android:textStyle="bold" />

                <CheckBox
                    android:id="@+id/check7"
                    android:layout_width="100dp"
                    android:layout_height="40dp"
                    android:text="Beet Green"
                    android:textColor="#ffffff"
                    android:textSize="10sp"
                    android:textStyle="bold" />

                <CheckBox
                    android:id="@+id/check8"
                    android:layout_width="100dp"
                    android:layout_height="40dp"
                    android:text="Swiss Chard"
                    android:textColor="#ffffff"
                    android:textSize="10sp"
                    android:textStyle="bold" />


                <CheckBox
                    android:id="@+id/check9"
                    android:layout_width="100dp"
                    android:layout_height="40dp"
                    android:text="Chienese Cabbage"
                    android:textColor="#ffffff"
                    android:textSize="10sp"
                    android:textStyle="bold" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textview_price"
                    android:textAlignment="center"
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
